class Multiplicar {
    // Constructor para multiplicar dos int
    Multiplicar(int a, int b) {
        int resultado = a * b;
        System.out.println("Multiplicación de dos int: " + resultado);
    }

    // Constructor para multiplicar tres int
    Multiplicar(int a, int b, int c) {
        int resultado = a * b * c;
        System.out.println("Multiplicación de tres int: " + resultado);
    }

    // Constructor para multiplicar dos double
    Multiplicar(double a, double b) {
        double resultado = a * b;
        System.out.println("Multiplicación de dos double: " + resultado);
    }
}
